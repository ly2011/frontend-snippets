{
  "name": "frontend-snippets",
  "displayName": "frontend-snippets",
  "description": "frontend-snippets",
  "version": "0.1.2",
  "license": "MIT",
  "publisher": "win",
  "author": "ly2011",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ly2011/frontend-snippets.git"
  },
  "bugs": {
    "url": "https://github.com/ly2011/frontend-snippet/issues",
    "email": "1518550424@qq.com"
  },
  "keywords": [
    "Vue",
    "Vue3",
    "Vue2",
    "Composition API",
    "Vue Snippets",
    "React",
    "React Hooks"
  ],
  "categories": [
    "Snippets"
  ],
  "scripts": {
    "build": "vsce package -o frontend-snippets$npm_package_version.vsix",
    "deploy": "vsce publish patch"
  },
  "contributes": {
    "languages": [
      {
        "id": "vue",
        "aliases": [
          "Vue",
          "vue"
        ],
        "extensions": [
          ".vue"
        ]
      },
      {
        "id": "javascript",
        "aliases": [
          "Vue",
          "vue"
        ],
        "extensions": [
          ".js",
          ".vue",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      },
      {
        "id": "javascript",
        "aliases": [
          "React"
        ],
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "html",
        "path": "./snippets/vue.json"
      },
      {
        "language": "vue",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/react.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/react.json"
      }
    ]
  }
}
